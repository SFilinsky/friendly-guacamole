{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es6",
    "outDir": "./lib",
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "strict": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
  "types": [
    "babylonjs"
  ]
}
